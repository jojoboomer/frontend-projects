{
  "extends": "astro/tsconfigs/strict",
  "include":  ["src", "styled-system",".astro/types.d.ts" , "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["@pandacss/dev"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@styled-system/*": ["styled-system/*"],
    }
  }
}